{
	"name": "@leanup/kolibri-schema",
	"version": "1.0.5",
	"license": "EUPL-1.2",
	"homepage": "https://github.com/leanupjs/kolibri#readme",
	"repository": "https://github.com/leanupjs/kolibri",
	"bugs": {
		"url": "https://github.com/leanupjs/kolibri/issues",
		"email": "github@martinoppitz.com"
	},
	"author": {
		"name": "Martin Oppitz",
		"email": "github@martinoppitz.com"
	},
	"contributors": [
		{
			"name": "Martin Oppitz",
			"email": "github@martinoppitz.com"
		}
	],
	"browser": true,
	"description": "Das Schema-Modul beinhaltet die konkrete Spezifikation der Komponenten-Bibliothek.",
	"sideEffects": false,
	"keywords": [
		"accessibility",
		"accessible",
		"bitv",
		"framework",
		"library",
		"designsystem",
		"design",
		"system",
		"web components",
		"webcomponents",
		"aria",
		"wai",
		"axe",
		"custom elements",
		"styleguide",
		"style",
		"guide",
		"ui",
		"html",
		"css",
		"web",
		"a11y",
		"w3c",
		"webstandard",
		"wcag"
	],
	"scripts": {
		"build": "unbuild",
		"depcheck": "depcheck --ignore-bin-package --skip-missing --ignores=@types/*",
		"dev": "nodemon --exec unbuild --ext js,json,ts --watch src",
		"format": "prettier --check src",
		"lint": "tsc --noemit && eslint src",
		"prepack": "unbuild"
	},
	"dependencies": {
		"adopted-style-sheets": "1.1.3"
	},
	"devDependencies": {
		"@types/node": "ts5.3",
		"@typescript-eslint/eslint-plugin": "6.19.1",
		"@typescript-eslint/parser": "6.19.1",
		"eslint": "8.56.0",
		"eslint-plugin-no-loops": "0.3.0",
		"nodemon": "3.0.3",
		"typescript": "5.3.3",
		"unbuild": "1.2.1"
	},
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	]
}
