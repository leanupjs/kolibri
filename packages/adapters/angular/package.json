{
	"name": "@leanup/kolibri-angular",
	"version": "1.0.0-rc.0",
	"license": "EUPL-1.2",
	"homepage": "https://github.com/leanupjs/kolibri#readme",
	"repository": "https://github.com/leanupjs/kolibri",
	"bugs": {
		"url": "https://github.com/leanupjs/kolibri/issues",
		"email": "github@martinoppitz.com"
	},
	"contributors": [
		{
			"name": "Martin Oppitz",
			"email": "github@martinoppitz.com"
		}
	],
	"browser": true,
	"description": "Das Angular-Modul ist der Framework-Adapter fÃ¼r die Komponenten-Bibliothek.",
	"sideEffects": false,
	"scripts": {
		"clean": "lerna exec --stream -- git clean -f -d -X && git clean -f -d -X",
		"build:cjs": "tsc -m commonjs --outDir cjs",
		"build:clean": "rimraf cjs esm types umd",
		"build:esm": "tsc -m esnext --outDir esm",
		"build:ngc": "node ng-module.js && ngc -p .",
		"build:tsc": "tsc -p .",
		"build:types": "tsc -d --outDir types && rimraf types/*.js types/*.map types/**/*.js types/**/*.map",
		"build:umd": "tsc -m umd --outDir umd",
		"build": "npm run build:clean && npm run build:ngc",
		"prepack": "npm run build",
		":install": "pnpm install",
		":publish": "npm unpublish -f --registry=http://localhost:4873 && npm publish --registry=http://localhost:4873"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "types/index.d.ts",
	"exports": {
		"require": "./cjs/index.js",
		"import": "./esm/index.js"
	},
	"devDependencies": {
		"@angular/compiler-cli": "14.0.6",
		"@angular/compiler": "14.0.6",
		"@angular/core": "14.0.6",
		"@kolibri/lib": "1.1.2-rc.1",
		"@leanup/kolibri-components": "1.0.0-rc.0",
		"@leanup/form": "1.3.23",
		"@types/minimatch": "3.0.5",
		"@types/minimist": "1.2.2",
		"@types/node": "18.0.4",
		"@types/normalize-package-data": "2.4.1",
		"prettier": "2.7.1",
		"rimraf": "3.0.2",
		"rxjs": "7.5.6",
		"tslib": "2.4.0",
		"typescript": "4.7.4",
		"zone.js": "0.11.6"
	},
	"peerDependencies": {
		"@angular/core": ">=14",
		"@leanup/kolibri-components": "1.0.0-rc.0"
	},
	"files": [
		"dist/"
	]
}
